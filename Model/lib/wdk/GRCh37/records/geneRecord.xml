<?xml version="1.0" encoding="UTF-8"?>
<wdkModel>
	<recordClassSet name="GeneRecordClasses">
		<recordClass
				name="GeneRecordClass"
				displayName="Gene"
				displayNamePlural="Genes"
				shortDisplayName="Gene"
				useBasket="false"
				urlName="gene">
			<!-- =================================================================== -->
			<!-- Primary Key -->
			<!-- =================================================================== -->
			<primaryKey aliasQueryRef="GeneAttributes.GeneAlias">
				<columnRef>source_id</columnRef>
			</primaryKey>

			<idAttribute
					name="primary_key"
					displayName="Gene">
				<text>
					<![CDATA[$$gene_symbol$$]]>
				</text>
			</idAttribute>

			<testParamValues>
				<paramValue name="source_id">ENSG00000130203</paramValue>
			</testParamValues>

			<!-- =================================================================== -->
			<!-- Reporters -->
			<!-- =================================================================== -->
			<reporter
					name="fullRecord"
					displayName="%%fullReporterDisplayName%%"
					scopes="record"
					implementation="org.gusdb.wdk.model.report.reporter.FullRecordReporter" />

			<reporter
					name="attributesTabular"
					displayName="%%attributesReporterDisplayName%%"
					scopes="results"
					implementation="org.gusdb.wdk.model.report.reporter.AttributesTabularReporter">
				<description> %%attributesReporterDescription%%</description>
				<property name="page_size">1000000</property>
			</reporter>

			<reporter
					name="tableTabular"
					displayName="%%tableReporterDisplayName%%"
					scopes="results"
					implementation="org.gusdb.wdk.model.report.reporter.TableTabularReporter">
				<description>%%tableReporterDescription%%</description>
				<property name="page_size">1000000</property>
			</reporter>

			<reporter
					name="xml"
					displayName="XML: choose from columns
					and/or tables"
					scopes=""
					implementation="org.gusdb.wdk.model.report.reporter.XMLReporter"></reporter>

			<!--<reporter name="json" displayName="JSON: choose from columns
					 and/or tables" scopes="record,results" implementation="org.gusdb.wdk.model.report.reporter.JSONReporter">
      		</reporter> -->


			<!-- =============================================================== -->
			<!-- Step Analysis Plugins -->
			<!-- =============================================================== -->
			<!-- <stepAnalysisRef name="custom-go-enrichment" /> -->
			<!-- <stepAnalysisRef name="pathway-enrichment"/> -->

			<!-- =============================================================== -->
			<!-- Summary and record views -->
			<!-- =============================================================== -->
			<summaryView
					name="_default"
					display="Gene Results">
				<description>
					<![CDATA[View gene details.]]>
				</description>
			</summaryView>

			<summaryView
					name="ideogram"
					display="Genome View">
				<description>
					<![CDATA[View your results mapped against the genome.]]>
				</description>
			</summaryView>

			<reporter
					name="genomeViewReporter"
					displayName="Ideogram"
					scopes=""
					implementation="org.niagads.genomics.model.report.summaryview.genomeview.GeneGenomeViewReporter" />

			<!-- =============================================================== -->
			<!-- Annotation and Link Outs -->
			<!-- =============================================================== -->
			<attributeQueryRef ref="GeneAttributes.BasicAttributes">
				<columnAttribute
						name="project_id"
						internal="true"
						inReportMaker="false" />
				<columnAttribute
						name="gene_type"
						displayName="Type" />
				<columnAttribute
						name="chromosome"
						displayName="Chromsome" />
				<columnAttribute
						name="location_start"
						displayName="Position (Start)" />
				<columnAttribute
						name="location_end"
						displayName="Position (End)" />
				<columnAttribute
						name="strand"
						displayName="Strand" />
				<columnAttribute
						name="is_reversed"
						internal="true"
						inReportMaker="false" />
				<columnAttribute
						name="gene_name"
						displayName="Product" />
				<columnAttribute
						name="synonyms"
						displayName="Synonyms" />
				<columnAttribute
						name="gene_symbol"
						displayName="Symbol" />
				<columnAttribute
						name="hgnc_id"
						displayName="HGNC ID" />
				<columnAttribute
						name="omim_id"
						displayName="OMIM ID" />
				<columnAttribute
						name="ucsc_id"
						displayName="UCSC ID" />
				<columnAttribute
						name="vega_id"
						displayName="VEGA ID" />
				<columnAttribute
						name="uniprot_id"
						displayName="UniProt ID(s)" />
				<columnAttribute
						name="entrez_id"
						displayName="NCBI Gene (Entrez) ID" />
				<columnAttribute
						name="ensembl_id"
						displayName="Ensembl ID" />
				<columnAttribute
						name="span"
						displayName="Location" />
				<columnAttribute
						name="cytogenetic_location"
						displayName="Cytogenetic Location" />
			</attributeQueryRef>

			<table
					name="record_link_outs"
					displayName="Related Gene Records"
					inReportMaker="false"
					queryRef="GeneTables.DbRefs_Gene">
				<columnAttribute
						name="external_id"
						displayName="External ID"
						internal="false" />
				<columnAttribute
						name="external_links"
						displayName="Resources"
						internal="false"
						truncateTo="5000" />

			</table>

			<table
					name="sequence_link_outs"
					displayName="Nucleotide Sequences"
					inReportMaker="false"
					queryRef="GeneTables.DbRefs_NSeq">
				<columnAttribute
						name="external_id"
						displayName="External ID"
						internal="false" />
				<columnAttribute
						name="external_links"
						displayName="Resources"
						internal="false"
						truncateTo="5000" />


			</table>

			<table
					name="protein_link_outs"
					displayName="Proteins"
					inReportMaker="false"
					queryRef="GeneTables.DbRefs_Proteins">
				<columnAttribute
						name="external_id"
						displayName="External ID"
						internal="false" />
				<columnAttribute
						name="external_links"
						displayName="Resources"
						internal="false"
						truncateTo="5000" />


			</table>

			<table
					name="clinical_link_outs"
					displayName="Clinical"
					inReportMaker="false"
					queryRef="GeneTables.DbRefs_Clinical">
				<columnAttribute
						name="external_id"
						displayName="External ID"
						internal="false" />
				<columnAttribute
						name="external_links"
						displayName="Resources"
						internal="false"
						truncateTo="5000" />


			</table>

			<table
					name="transcript_link_outs"
					displayName="Transcripts"
					queryRef="GeneTables.Transcripts">
				<columnAttribute
						name="transcript_name"
						displayName="Transcript" />
				<columnAttribute
						name="transcript_accession_link"
						displayName="Ensembl Transcript" />
				<columnAttribute
						name="location_start"
						displayName="Position (Start)" />
				<columnAttribute
						name="location_end"
						displayName="Position (End)" />


			</table>

			<!-- =============================================================== -->
			<!-- Genetic Variation -->
			<!-- =============================================================== -->

			<attributeQueryRef ref="GeneAttributes.GWSVariantsPlot">
				<columnAttribute
						name="gws_variants_summary_plot"
						internal="true"
						inReportMaker="false"></columnAttribute>
			</attributeQueryRef>

			<!-- <attributeQueryRef ref="GeneAttributes.ADEvidenceFlag">
	<columnAttribute name="has_genetic_evidence_for_ad_risk_display" displayName="Genetic Evidence for AD?" help="Genetic evidence from exonic rare-variant aggregation tests of association to AD have been reported for this gene by the ADSP" internal="true" inReportMaker="false"  />
	<columnAttribute name="has_genetic_evidence_for_ad_risk" displayName="Genetic Evidence for AD?" help="Genetic evidence from exonic rare-variant aggregation tests of association to AD have been reported for this gene by the ADSP" internal="true" inReportMaker="true"  />

      </attributeQueryRef> -->


			<!-- GWAS -->
			<table
					name="ad_variants_from_gwas"
					displayName="Alzheimer's Disease"
					queryRef="GeneTables.ADVariantsFromGWAS">
				<columnAttribute
						name="relative_position"
						displayName="Relative Position" />
				<columnAttribute
						name="variant_link"
						displayName="Variant" />
				<columnAttribute
						name="pvalue"
						displayName="p-value" />
				<columnAttribute
						name="allele"
						displayName="Test/Minor Allele" />
				<columnAttribute
						name="track_name_link"
						displayName="Track" />
				<columnAttribute
						name="track_description"
						displayName="Track Description" />
				<columnAttribute
						name="ref_snp_id"
						displayName="RefSNP ID" />
				<columnAttribute
						name="adsp_variant_flag"
						displayName="ADSP Variant?"
						help="variant identified by the ADSP and passed the ADSP QC protocol" />
				<columnAttribute
						name="gene_impact"
						displayName="Impact"
						help="Most severe impact predicted for this variant on the gene, using the ADSP Annotation Pipeline" />
				<columnAttribute
						name="gene_consequence"
						displayName="Consequence"
						help="Most severe consequence predicted for this variant on the gene, using the ADSP Annotation Pipeline" />
				<columnAttribute
						name="population"
						displayName="Population" />
				<columnAttribute
						name="biomarker"
						displayName="Biomarker" />
				<columnAttribute
						name="genotype"
						displayName="Genotype" />
				<columnAttribute
						name="gender"
						displayName="Gender" />
				<columnAttribute
						name="tissue"
						displayName="Tissue" />
				<columnAttribute
						name="covariates"
						displayName="Covariates" />

				<description>
					<![CDATA[
		   <p>Variants contained within &plusmn;100kb of this gene that have been identified in a NIAGADS GWAS 
		   summary statistics dataset for Alzheimer's disease.</p>

		   <p>By default, the table is filtered for variants whose risk-association is supported by a 
		   <strong><em>p</em>-value &lt; 5e<sup>-8</sup></strong>.</p>
	  ]]>
				</description>
			</table>

			<table
					name="other_variants_from_gwas"
					displayName="AD-related neuropathologies and biomarkers"
					queryRef="GeneTables.OtherVariantsFromGWAS">
				<columnAttribute
						name="relative_position"
						displayName="Relative Position" />
				<columnAttribute
						name="variant_link"
						displayName="Variant" />
				<columnAttribute
						name="pvalue"
						displayName="p-value" />
				<columnAttribute
						name="allele"
						displayName="Test/Minor Allele" />
				<columnAttribute
						name="track_name_link"
						displayName="Track" />
				<columnAttribute
						name="track_description"
						displayName="Track Description" />
				<columnAttribute
						name="ref_snp_id"
						displayName="RefSNP ID" />
				<columnAttribute
						name="adsp_variant_flag"
						displayName="ADSP Variant?"
						help="variant identified by the ADSP and passed the ADSP QC protocol" />
				<columnAttribute
						name="gene_impact"
						displayName="Impact"
						help="Most severe impact predicted for this variant on the gene, using the ADSP Annotation Pipeline" />
				<columnAttribute
						name="gene_consequence"
						displayName="Consequence"
						help="Most severe consequence predicted for this variant on the gene, using the ADSP Annotation Pipeline" />
				<columnAttribute
						name="population"
						displayName="Population" />
				<columnAttribute
						name="diagnosis"
						displayName="Diagnosis" />
				<columnAttribute
						name="neuropathology"
						displayName="Neuropathology" />
				<columnAttribute
						name="biomarker"
						displayName="Biomarker" />
				<columnAttribute
						name="genotype"
						displayName="Genotype" />
				<columnAttribute
						name="gender"
						displayName="Gender" />
				<columnAttribute
						name="tissue"
						displayName="Tissue" />
				<columnAttribute
						name="covariates"
						displayName="Covariates" />


				<description>
					<![CDATA[
						<p>Variants contained within &plusmn;100kb of this gene that have been identified in a NIAGADS GWAS summary statistics 
							dataset for an AD-related disease or neuropathology.</p>
						<p>By default, the table is filtered for variants whose risk-association is supported by a <strong><em>p</em>-value &lt; 5e<sup>-8</sup></strong>.</p>
	  				]]>
				</description>
			</table>

			<table
					name="ad_variants_from_catalog"
					displayName="Alzheimer's Disease"
					queryRef="GeneTables.ADVariantsFromCatalog">
				<columnAttribute
						name="source"
						displayName="Data Source"
						help="NHGRI or NIAGADS-curated GWAS Catalog" />
				<columnAttribute
						name="relative_position"
						displayName="Relative Position" />
				<columnAttribute
						name="variant_link"
						displayName="Variant" />
				<columnAttribute
						name="ref_snp_id"
						displayName="RefSNP ID" />
				<columnAttribute
						name="pvalue"
						displayName="p-value" />
				<columnAttribute
						name="mapped_efo_trait"
						displayName="Trait"
						help="standardized trait; mapped to ontology" />
				<columnAttribute
						name="trait"
						displayName="Trait (Reported)" />

				<columnAttribute
						name="pubmed_id"
						displayName="PubMed ID" />
				<columnAttribute
						name="study"
						displayName="Study" />
				<columnAttribute
						name="adsp_variant_flag"
						displayName="ADSP Variant?"
						help="variant identified by the ADSP and passed the ADSP QC protocol" />
				<columnAttribute
						name="sample"
						displayName="Sample" />
				<columnAttribute
						name="replicate_sample"
						displayName="Replicate (Stage 2) Sample" />
				<columnAttribute
						name="allele"
						displayName="Test/Minor Allele" />
				<columnAttribute
						name="frequency"
						displayName="Frequency" />

				<columnAttribute
						name="gene_impact"
						displayName="Impact"
						help="Most severe impact predicted for this variant on the gene, using the ADSP Annotation Pipeline" />
				<columnAttribute
						name="gene_consequence"
						displayName="Consequence"
						help="Most severe consequence predicted for this variant on the gene, using the ADSP Annotation Pipeline" />


				<description>
					<![CDATA[    
                   <p>Variants contained within &plusmn;10kb of this gene from curated collections (NHGRI or NIAGADS) of SNPs 
		   identified as significantly associated with Alzheimer's disease in published 
                   genome-wide association studies (GWAS).</p>
          ]]>
				</description>
			</table>


			<table
					name="other_variants_from_catalog"
					displayName="Other Traits (incl. related neuropathologies and AD biomarkers)"
					queryRef="GeneTables.OtherVariantsFromCatalog">
				<columnAttribute
						name="source"
						displayName="Data Source"
						help="NHGRI or NIAGADS-curated GWAS Catalog" />
				<columnAttribute
						name="relative_position"
						displayName="Relative Position" />
				<columnAttribute
						name="variant_link"
						displayName="Variant" />
				<columnAttribute
						name="ref_snp_id"
						displayName="RefSNP ID" />
				<columnAttribute
						name="pvalue"
						displayName="p-value" />
				<columnAttribute
						name="mapped_efo_trait"
						displayName="Trait"
						help="standardized trait; mapped to ontology" />
				<columnAttribute
						name="trait"
						displayName="Trait (Reported)" />

				<columnAttribute
						name="pubmed_id"
						displayName="PubMed ID" />
				<columnAttribute
						name="study"
						displayName="Study" />
				<columnAttribute
						name="adsp_variant_flag"
						displayName="ADSP Variant?"
						help="variant identified by the ADSP and passed the ADSP QC protocol" />
				<columnAttribute
						name="sample"
						displayName="Sample" />
				<columnAttribute
						name="replicate_sample"
						displayName="Replicate (Stage 2) Sample" />
				<columnAttribute
						name="allele"
						displayName="Test/Minor Allele" />
				<columnAttribute
						name="frequency"
						displayName="Frequency" />

				<columnAttribute
						name="gene_impact"
						displayName="Impact"
						help="Most severe impact predicted for this variant on the gene, using the ADSP Annotation Pipeline" />
				<columnAttribute
						name="gene_consequence"
						displayName="Consequence"
						help="Most severe consequence predicted for this variant on the gene, using the ADSP Annotation Pipeline" />


				<description>
					<![CDATA[   
                   <p>Variants contained within &plusmn;10kb of this gene from curated collections (NHGRI or NIAGADS) of SNPs identified 
		   as significantly associated with any disease/trait, including AD-related neuropathologies and AD biomarkers in published
                   genome-wide association studies (GWAS)</p>   
          ]]>
				</description>

			</table>

