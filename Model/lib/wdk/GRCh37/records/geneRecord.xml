			<!-- =============================================================== -->
			<!-- Functional Annotation -->
			<!-- =============================================================== -->

			<!-- 	<columnAttribute displayName="Association" name="evidence_table_dropdown" help="click on links to reveal supporting evidence for this association"  /> -->
			<table
					name="go_terms"
					displayName="Gene Ontology"
					queryRef="GeneTables.GoTerms">

				<columnAttribute
						displayName="Term"
						name="term" />
				<columnAttribute
						displayName="GO Accession"
						name="go_accession"
						help="follow link for more information from AmIGO" />
				<columnAttribute
						displayName="Evidence Code"
						name="go_evidence_code" />
				<columnAttribute
						displayName="Ontology"
						name="ontology" />


				<description>
					<![CDATA[
                   <p>Annotations of gene function from the Gene Ontology Consortium.<br/>
                   Mouse over term names to view a full definition.<br/>
                   Click on the <span class="blue">GO Accession</sp> link to view the term in the GO hierarchy on the AMIGO website.<br/>
	  ]]>
				</description>
			</table>

			<table
					name="pathways"
					displayName="Pathways"
					queryRef="GeneTables.Pathways">
				<columnAttribute
						name="pathway_name"
						displayName="Pathway" />
				<columnAttribute
						name="accession"
						displayName="Accession"
						truncateTo="5000"
						help="view pathway details at KEGG or Reactome" />
				<!--
	    <columnAttribute name="pathway_image" displayName="Graph" truncateTo="5000"  help="click image thumbnail to view gene highlighted in the pathway on the KEGG or Reactome website"/>
	-->
				<columnAttribute
						name="source"
						displayName="Resource" />


				<description>
					<![CDATA[
		   <p>Gene-pathway membership from KEGG and Reactome.</p>
		   <p>Click on the <span class="blue">Pathway Accession</sp> link to view more information about the pathway on the KEGG or Reactome website.</p>
	  ]]>
				</description>
			</table>

		</recordClass>
	</recordClassSet>
</wdkModel>