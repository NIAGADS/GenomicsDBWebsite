      <!-- =============================================================== -->
      <!-- Genetic Variation  -->
      <!-- =============================================================== -->

      <attributeQueryRef ref="GwasDatasetAttributes.TopGenes">
	<columnAttribute name="top_gene_list" displayName="Top Genes" type="json" internal="true" inReportMaker="false"/>
      </attributeQueryRef>

