<?xml version="1.0" encoding="UTF-8"?>
<wdkModel>
  <recordClassSet name="DatasetRecordClasses">
    <recordClass name="ResourceDatasetRecordClass" displayName="Data Source"
		 displayNamePlural="Data Sources" shortDisplayName="Data Source"
		 useBasket="false" urlName="resource">
      <!-- =================================================================== -->
      <!-- Primary Key -->
      <!-- =================================================================== -->

      <primaryKey aliasPluginClassName="org.gusdb.wdk.model.record.GenericRecordPrimaryKeyAliasPlugin">
	<columnRef>
	  resource_id
	</columnRef>
      </primaryKey>

      <idAttribute name="primary_key" displayName="Resource ID">
	<text>
	  <![CDATA[$$resource_id$$]]>
	</text>
      </idAttribute>

      <testParamValues>
	<paramValue name="resource_id">
	  NG00027
	</paramValue>
      </testParamValues>

      <!-- =================================================================== -->
      <!-- Reporters -->
      <!-- =================================================================== -->
      <reporter name="fullRecord"
		displayName="%%fullReporterDisplayName%%"
		scopes="record"
		implementation="org.gusdb.wdk.model.report.reporter.FullRecordReporter" />

      <reporter name="attributesTabular"
		displayName="%%attributesReporterDisplayName%%"
		scopes="results"
		implementation="org.gusdb.wdk.model.report.reporter.AttributesTabularReporter">
	<description>
	  %%attributesReporterDescription%%
	</description>
	<property name="page_size">
	  1000000
	</property>
      </reporter>

      <reporter name="tableTabular"
		displayName="%%tableReporterDisplayName%%"
		scopes="results"
		implementation="org.gusdb.wdk.model.report.reporter.TableTabularReporter">
	<description>
	  %%tableReporterDescription%%
	</description>
	<property name="page_size">
	  1000000
	</property>
      </reporter>

      <reporter name="xml" displayName="XML: choose from columns
					and/or tables" scopes=""
		implementation="org.gusdb.wdk.model.report.reporter.XMLReporter">
      </reporter>

      <reporter name="json" displayName="JSON: choose from columns
					 and/or tables" scopes="record,results"
		implementation="org.gusdb.wdk.model.report.reporter.JSONReporter">
      </reporter>

      <!-- =============================================================== -->
      <!-- Step Analysis Plugins -->
      <!-- =============================================================== -->

      <!-- <stepAnalysisRef name="pathway-enrichment"/> -->

      <!-- =============================================================== -->
      <!-- Annotation and Link Outs -->
      <!-- =============================================================== -->
      <attributeQueryRef ref="ResourceAttributes.BasicAttributes">
	<columnAttribute name="project_id" internal="true" inReportMaker="false" type="string" />
	<columnAttribute name="accession_link" displayName="Accession" type="json_link"/>
	<columnAttribute name="name" displayName="Name" type="string"/>
	<columnAttribute name="description" displayName="Description" type="string"/>
	<columnAttribute name="attribution" displayName="Attribution" type="string" />
	<columnAttribute name="version" displayName="Version" type="string"/>
	<columnAttribute name="category" displayName="Category" type="string"/>
	<columnAttribute name="caveats" displayName="Notes/Caveats" type="string"/>
	<columnAttribute name="is_adsp" displayName="ADSP?" type="json_icon"/>
      </attributeQueryRef>

      <!-- =============================================================== -->
      <!-- Tracks / Datasets -->
      <!-- =============================================================== -->

    </recordClass>

    <recordClass name="NIAGADSDatasetRecordClass" displayName="NIAGADS Dataset"
		 displayNamePlural="NIAGADS Datasets"
		 shortDisplayName="Dataset" useBasket="false" urlName="dataset">
      <!-- =================================================================== -->
      <!-- Primary Key -->
      <!-- =================================================================== -->

      <primaryKey aliasPluginClassName="org.gusdb.wdk.model.record.GenericRecordPrimaryKeyAliasPlugin">
	<columnRef>
	  dataset_type
	</columnRef>
      </primaryKey>

      <idAttribute name="primary_key" displayName="Type">
	<text>
	  <![CDATA[$$dataset_type$$]]>
	</text>
      </idAttribute>


      <!-- =================================================================== -->
      <!-- Reporters -->
      <!-- =================================================================== -->
      <reporter name="fullRecord"
		displayName="%%fullReporterDisplayName%%"
		scopes="record"
		implementation="org.gusdb.wdk.model.report.reporter.FullRecordReporter" />

      <reporter name="attributesTabular"
		displayName="%%attributesReporterDisplayName%%"
		scopes="results"
		implementation="org.gusdb.wdk.model.report.reporter.AttributesTabularReporter">
	<description>
	  %%attributesReporterDescription%%
	</description>
	<property name="page_size">
	  1000000
	</property>
      </reporter>

      <reporter name="tableTabular"
		displayName="%%tableReporterDisplayName%%"
		scopes="results"
		implementation="org.gusdb.wdk.model.report.reporter.TableTabularReporter">
	<description>
	  %%tableReporterDescription%%
	</description>
	<property name="page_size">
	  1000000
	</property>
      </reporter>

      <reporter name="xml" displayName="XML: choose from columns
					and/or tables"
		scopes=""
		implementation="org.gusdb.wdk.model.report.reporter.XMLReporter">
      </reporter>

      <reporter name="json" displayName="JSON: choose from columns
					 and/or tables"
		scopes="record,results"
		implementation="org.gusdb.wdk.model.report.reporter.JSONReporter">
      </reporter>

      <!-- =============================================================== -->
      <!-- Step Analysis Plugins -->
      <!-- =============================================================== -->


      <!-- =============================================================== -->
      <!-- Annotation and Link Outs -->
      <!-- =============================================================== -->
      <attributeQueryRef ref="NIAGADSDatasetAttributes.BasicAttributes">
	<columnAttribute name="project_id" internal="true" inReportMaker="false" type="string" />
	<columnAttribute name="description" displayName="Description" type="string"/>
	<columnAttribute name="name" displayName="Name" type="string"/>
      </attributeQueryRef>

      <table name="datasets" displayName="Datasets" queryRef="NIAGADSDatasetTables.Datasets">
	<columnAttribute name="dataset_accession" displayName="NIAGADS Accession" type="string"/>
	<columnAttribute name="consortium" displayName="Consortium" type="string" />
		
	<columnAttribute name="track" displayName="Track" type="string" internal="true" inReportMaker="true"/>
	<columnAttribute name="name" displayName="Name" type="string" internal="true"
			 inReportMaker="true"/>
	<columnAttribute name="dataset_record_link" displayName="Name" type="json_link"  inReportMaker="false"/>

	<columnAttribute name="attribution" displayName="Attribution" type="string" />
	<columnAttribute name="description" displayName="Description" type="string" />
	<columnAttribute name="neuropathology" displayName="Diagnosis/Neuropathology" type="string" />
	<columnAttribute name="population" displayName="Population" type="string"/>
	<columnAttribute name="biomarker" displayName="Biomarker" type="string"/>
	<columnAttribute name="additional_phenotype" displayName="Additional Phenotypes"
			 type="string"
			 help="additional phenotypic information, incl. APOE Carrier Status" />
	<columnAttribute name="covariates" displayName="Covariates" type="string" />

	<propertyList name="type">
	  <value>select_filter</value>
	</propertyList>
	<propertyList name="filter_field">
	  <value>dataset_accession,consortium,neuropathology,population,biomarker</value>
	</propertyList>
	<propertyList name="defaultOpen">
	  <value>true</value>
	</propertyList>
	   
	
    </table>

    </recordClass>


    <recordClass name="GWASDatasetRecordClass"
		 displayName="Summary Statistics Dataset Report"
		 displayNamePlural="Datasets" shortDisplayName="Dataset"
		 useBasket="false" urlName="gwas_summary">

      <!-- =================================================================== -->
      <!-- Primary Key -->
      <!-- =================================================================== -->

      <primaryKey aliasPluginClassName="org.gusdb.wdk.model.record.GenericRecordPrimaryKeyAliasPlugin">
	<columnRef>
	  track
	</columnRef>
      </primaryKey>

      <idAttribute name="primary_key" displayName="Dataset" help="explore this dataset">
	<text>
	  <![CDATA[$$name$$]]>
	</text>
      </idAttribute>

      <testParamValues>
	<paramValue name="track">
	  NG00027_STAGE1
	</paramValue>
      </testParamValues>


      <!-- =================================================================== -->
      <!-- Reporters -->
      <!-- =================================================================== -->
      <reporter name="fullRecord" displayName="%%fullReporterDisplayName%%"
