displayName: NIAGADS Accession Lookup

get: 
    description: > 
        Find data tracks for a specific NIAGADS Accession that are available to query or browse through the GenomicsDB
    queryParameters:
        accession: 
            description: NIAGADS accession 
            example: NG00041
            required: true
            
    responses:
        200:
            body:
                application/json:
                    example: !include examples/NG00041.json
